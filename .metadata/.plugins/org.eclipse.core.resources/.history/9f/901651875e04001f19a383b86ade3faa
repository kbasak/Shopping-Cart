package com.cts.shopping.cart.response;

import com.cts.shopping.cart.constants.Constants;
import com.cts.shopping.cart.constants.Constants.ApplicationError;
import com.cts.shopping.cart.constants.Constants.ResponseStatus;

public class BaseResponse {
	private Constants.ResponseStatus status_code;
	private Constants.ApplicationError validation_error_code;
	private String message;

	public Constants.ResponseStatus getStatus_code() {
		return status_code;
	}

	public void setStatus_code(Constants.ResponseStatus status_code) {
		this.status_code = status_code;
	}

	public Constants.ApplicationError getValidation_error_code() {
		return validation_error_code;
	}

	public void setValidation_error_code(Constants.ApplicationError validation_error_code) {
		this.validation_error_code = validation_error_code;
	}

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}

	public BaseResponse(ResponseStatus status_code, ApplicationError validation_error_code, String message) {
		super();
		this.status_code = status_code;
		this.validation_error_code = validation_error_code;
		this.message = message;
	}

}
