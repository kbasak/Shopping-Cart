package com.cts.shopping.cart.response;

import com.cts.shopping.cart.constants.Constants.ApplicationError;
import com.cts.shopping.cart.constants.Constants.ResponseStatus;

public class RegistrationResponse extends BaseResponse {
	private String username;
	private String password;

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public RegistrationResponse(ResponseStatus status_code, ApplicationError validation_error_code, String message,
			String username, String password) {
		super(status_code, validation_error_code, message);
		this.username = username;
		this.password = password;
	}

	public RegistrationResponse(ResponseStatus status_code, ApplicationError validation_error_code, String message) {
		super(status_code, validation_error_code, message);
	}

}
